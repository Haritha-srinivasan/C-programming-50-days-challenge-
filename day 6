#include<stdio.h> 

int test(int x, int y, int z)
{
   
if ((x>=y && x<=z)||(x<=y && x>=z)) 
return 1; 
else
return 0; 
}

int main()
{
int x, y, z; 
printf("Input the first integer: ");

    
scanf("%d", &x);

printf("Input the second integer: ");

   
scanf("%d", &y);

    
printf("Input the third integer: ");

scanf("%d", &z);

printf("\n");

printf("Second largest number among said three integers: ");

    
if (test(x,y,z))
printf("%d\n",x);
else if (test(y,x,z)) 
printf("%d\n",y);
else
printf("%d\n",z); 

return 0;
} 
